        "Fazendo o meio de colocar a imagem"
      ],
      "metadata": {
        "id": "HQYrmrhEVxlw"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import cv2\n",
        "import pytesseract\n",
        "from google.colab import files\n",
        "from PIL import Image\n",
        "\n",
        "# Faz o upload da imagem\n",
        "uploaded = files.upload()\n",
        "\n",
        "# Carrega a imagem utilizando o OpenCV\n",
        "image_path = list(uploaded.keys())[0]\n",
        "image = cv2.imread(image_path)\n",
        "\n",
        "# Converte a imagem para escala de cinza\n",
        "gray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)\n",
        "\n",
        "# Aplica um pré-processamento, como a remoção de ruído\n",
        "gray = cv2.GaussianBlur(gray, (5, 5), 0)\n",
        "\n",
        "# Utiliza o pytesseract para realizar o OCR na imagem\n",
        "imagem = pytesseract.image_to_string(Image.fromarray(gray))\n"
      ],
      "metadata": {
        "colab": {
